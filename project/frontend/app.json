{
  "expo": {
    "name": "FoodiesHub",
    "slug": "frontend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/foodieshub.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/d3a7178e-c603-44ab-87d9-d5f252944c9f"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foodieshub.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.foodieexpress.app",
      "versionCode": 2,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload menu item images."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "d3a7178e-c603-44ab-87d9-d5f252944c9f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
